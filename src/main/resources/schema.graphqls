type Notes {
    id: String,
    title: String,
    content: String,
    creator:String,
    isDeleted: Int,
    lastEditTime: Date,
    createDate: Date
}

input PostInput{
    title: String!,
    content: String!
}

type Query{
    posts: [Post]
    post(id: ID!): Post
}

type Mutation{
    createPost(post: PostInput): Post!
    updatePost(id: ID!, post: PostInput): Post!
    deletePost(id: ID!): String
}